{
  "build": {
    "env": {
      "HUGO_VERSION": "0.81.0"
    }
  },
    "headers": [ { 
      "source": "/(.*)", 
      "headers" : [ 
        { "key" : "X-Content-Type-Options",
        "value" : "nosniff"
        }, 
        { "key" : "X-Frame-Options", 
        "value" : "DENY" 
        }, 
        { "key" : "X-XSS-Protection",
        "value" : "1; mode=block" 
        } 
      ] 
    }
    ] 
}
